<h1>Editing an additional location</h1>

<% form_for(@location) do |f| %>
  <%= f.error_messages %>

  <p>
  <table>
    <tr>
      <td>Address Line 1:</td>
      <td><%= f.text_field :address_line_1 %></td>
    </tr>
    <tr>
      <td>Address Line 2:</td>
      <td><%= f.text_field :address_line_2 %></td>
    </tr>
    <tr>
      <td>City:</td>
      <td><%= f.text_field :city %></td>
    </tr>
    <tr>
      <td>State:</td>
      <td><%= f.text_field :state %></td>
    </tr>
    <tr>
      <td>Zip:</td>
      <td><%= f.text_field :zip %></td>
    </tr>
    <tr>
      <td>Phone:</td>
      <td><%= f.text_field :phone %></td>
    </tr>
    <tr>
      <td>Phone 2:</td>
      <td><%= f.text_field :phone_2 %></td>
    </tr>
  </table>
  <%= f.submit "Update" %>

<% end %>

<%= link_to 'Back to this Business', @business %>
